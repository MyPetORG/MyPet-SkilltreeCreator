{
  "name": "skilltree-creator",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --configuration=proxy --open",
    "build": "ng build",
    "build:website": "ng build --prod --configuration=website",
    "build:plugin": "ng build --prod --configuration=plugin --no-progress",
    "lint": "ng lint",
    "pre-build": "node -e \"console.log('export const APP_VERSION = \\'' + process.env.npm_package_version.substring(0,process.env.npm_package_version.lastIndexOf('.')) + '-b' + (process.env.npm_config_BUILD || '0') +'\\';')\" > src/version.ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "9.0.2",
    "@angular/compiler": "9.0.2",
    "@angular/core": "9.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "9.0.2",
    "@angular/platform-browser-dynamic": "9.0.2",
    "@angular/platform-server": "9.0.2",
    "@angular/router": "9.0.2",
    "@datorama/akita": "^4.19.4",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ncstate/sat-popover": "^4.0.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sentry/browser": "^5.12.5",
    "angular2-hotkeys": "2.1.5",
    "core-js": "^2.6.2",
    "lodash": "^4.17.15",
    "ngx-clipboard": "^13.0.0",
    "ngx-contextmenu": "^5.3.0",
    "pako": "^1.0.11",
    "rxjs": "^6.5.4",
    "subsink": "^1.0.1",
    "tslib": "^1.11.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "9.0.2",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
